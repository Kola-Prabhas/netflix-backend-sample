{
	"version": 2,
	"builds": [
		{
			"src": "server.js",
			"use": "@vercel/node"
		},
		{
			"src": "dist/**",
			"use": "@vercel/static"
		}
	],
	"rewrites": [
		{
			"source": "/payment-success/:id",
			"destination": "/"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": {
				"Cache-Control": "s-maxage=31536000, immutable"
			}
		}
	]
}